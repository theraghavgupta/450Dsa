#include<bits/stdc++.h>
using namespace std;

void rotate(int arr[], int n){
    int high = arr[n-1];
    for(int i=n-1;i>=0;i--){
        arr[i] = arr[i-1];
    }
    arr[0]=high;
}

void twoPointer(int arr[], int n){
    int low=0,high=n-1;
    while(low<=high){
        swap(arr[low],arr[high]);
        low++;
    }
}

int main(){
    int a[] = {1,2,3,4,5};
    int n = sizeof(a)/sizeof(int);    
    
    rotate(a,n);
    twoPointer(a,n);
    
    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
}
