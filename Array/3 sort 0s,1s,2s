#include<bits/stdc++.h>
using namespace std;

void countAr(int arr[],int n,int count[]){
    for(int i=0;i<n;i++){
        if(arr[i]==0){
            count[0]++;
        }else if(arr[i]==1){
            count[1]++;
        }else{
            count[2]++;
        }
    }
    for(int i=0;i<count[0];i++){
        arr[i]=0;
    }for(int i=count[0];i<(count[0]+count[1]);i++){
        arr[i]=1;
    }for(int i=(count[0]+count[1]);i<(count[0]+count[1]+count[2]);i++){
        arr[i]=2;
    }
    
}

int main(){
    //sorting the array takes O(nlogn) time using vector sort
    // vector<int> S = {1,1,0,2,2,1,0};
    // sort(S.begin(),S.end());
    // for(int x:S){
    //     cout<<x<<" ";
    // }
    
    //Linear Time O(n) to sort 0,1,2
    int arr[7] = {1,1,0,2,2,1,0};
    int n = sizeof(arr)/sizeof(int);
    int count[3] = {0,0,0};
    countAr(arr,n,count);
    
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    
}
